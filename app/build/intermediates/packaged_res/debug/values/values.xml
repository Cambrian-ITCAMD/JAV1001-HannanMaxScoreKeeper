<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="background">#130c1e</color>
    <color name="black">#FF000000</color>
    <color name="container_background">#221539</color>
    <color name="fonts">#f9156e</color>
    <color name="ic_launcher_background">#FFFFFF</color>
    <color name="logo_fonts">#70243c</color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="white">#FFFFFFFF</color>
    <string name="app_name">HannanMaxScoreKeeper</string>
    <string name="app_name_secondary">Hannan Max\nScore Keeper</string>
    <string name="enter_team_1_name">Enter Team 1 Name</string>
    <string name="enter_team_2_name">Enter Team 2 Name</string>
    <style name="SwitchTextAppearance" parent="@android:style/TextAppearance.Holo.Small">
        <item name="android:textColor">@drawable/text_color_selector</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="Theme.AppCompat.DayNight.NoActionBar">
        
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
</resources>